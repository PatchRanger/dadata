{
    "name": "magdv/dadata",
    "type": "library",
    "description": "Data cleansing, enrichment and suggestions via Dadata API",
    "homepage": "https://github.com/magdv/dadata",
    "license": "MIT",
    "authors": [
        {
            "name": "Korolev Denis",
            "email": "d.korolev@magdv.com"
        }
    ],
    "keywords": [
        "dadata",
        "suggestions",
        "api",
        "php"
    ],
    "require": {
        "php": "^7.4",
        "psr/http-client": "^1.0",
        "nyholm/psr7": "^1.5"
    },
    "require-dev": {
        "rector/rector": "^0.15.17",
        "ext-json": "*",
        "overtrue/phplint": "^2.0",
        "phpunit/phpunit": "^8.5",
        "squizlabs/php_codesniffer": "^3.5",
        "vimeo/psalm": "^3.8",
        "guzzlehttp/guzzle": "^7.5"
    },
    "autoload": {
        "psr-4": {
            "MagDv\\Dadata\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MagDv\\Dadata\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "lint": "phplint",
        "cs-check": "phpcs",
        "cs-fix": "phpcbf",
        "psalm": "psalm",
        "test": "phpunit --colors=always"
    }
}
